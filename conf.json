{
    "plugins": [
        "plugins/markdown"
    ],
    "title": "",
    "recurseDepth": 10,
    "source": {
        "include": [
            "./src/game_scaler.js",
            "./src/game_physics.js",
            "./src/game_context.js",
            "./src/game_config.js",
            "./src/scenes/main_game_scene.js",
            "./doc/Home.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "staticFiles": {
                "paths": [
                    "doc/"
                ]
            }
        }
    }
}